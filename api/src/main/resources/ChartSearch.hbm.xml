<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.chartsearch.solr.nonPatient">

	<class name="SearchProject" table="${project.parent.artifactId}_project">
		<id name="projectId" type="int" column="project_id" unsaved-value="0">
			<generator class="native" />
		</id>
		<discriminator column="project_id" insert="false" />

		<property name="projectName" type="java.lang.String" column="name"
			length="255" unique="true" />
		<property name="projectUuid" type="java.lang.String" column="uuid"
			length="38" unique="true" />
		<property name="projectDescription" type="java.lang.String"
			column="description" length="255" />
		<property name="sqlQuery" type="java.lang.String" column="sql_query" />
		<property name="columnNames" type="java.lang.String" column="column_names" />
	</class>

</hibernate-mapping>
